{
	"name": "eleventy-multisite",
	"version": "0.0.4",
	"description": "Eleventy plugin adding multi-site support",
	"keywords": [
		"eleventy-plugin",
		"multisite"
	],
	"main": "index.js",
	"repository": "https://github.com/bnoctis/eleventy-multisite",
	"author": "Blair Noctis <code@noctis.im>",
	"license": "BSD-3-Clause",
	"bin": "cmd.js",
	"scripts": {
		"build": "yarn run tsc",
		"test": "yarn run tsc && yarn run ava",
		"postinstall": "yarn run husky install",
		"prepack": "pinst --disable",
		"postpack": "pinst --enable"
	},
	"dependencies": {
		"@11ty/eleventy": "^1.0.1",
		"@types/command-exists": "^1.2.0",
		"command-exists": "^1.2.9",
		"debug": "^4.3.4",
		"fast-glob": "^3.2.11",
		"ignore": "^5.2.0",
		"minimatch": "^5.1.0",
		"minimist": "^1.2.6"
	},
	"devDependencies": {
		"@ava/typescript": "^3.0.1",
		"@types/debug": "^4.1.7",
		"@types/glob": "^7.2.0",
		"@types/minimatch": "^3.0.5",
		"@types/minimist": "^1.2.2",
		"@types/node": "^17.0.35",
		"ava": "^4.2.0",
		"husky": "^8.0.0",
		"pinst": "^3.0.0",
		"typescript": "^4.6.4"
	},
	"packageManager": "yarn@3.2.1"
}
